{
    "manifest_version": 2,
    "name": "Node Travel Scrambler",
    "version": "0.1",
    "content_scripts": [{
        "matches": [
            "*://*.mozilla.org/*",
            "*://*.hslu.ch/*"
        ],
        "js": ["scrambler.js"]
    }],
    "icons": {
        "48": "icons/scrambler48.png",
        "96": "icons/scrambler96.png"
      },
      "browser_action": {
        "default_icon": "icons/scrambler96.png"
    }
}